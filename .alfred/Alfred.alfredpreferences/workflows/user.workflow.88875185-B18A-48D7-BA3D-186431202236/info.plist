} 
if (index(lc('Modify Sites Configuration'), lc("{query}")) &amp;&amp; "{query}" ne "") {
print qq{&lt;item uid="0001"&gt;&lt;arg&gt;sitesconfig&lt;/arg&gt;&lt;title&gt;Modify sites configuration&lt;/title&gt;&lt;subtitle&gt;&lt;/subtitle&gt;&lt;icon&gt;images/sites_config.png&lt;/icon&gt;&lt;/item&gt;\n};}
if ((index(lc('Open configuration folder'), lc("{query}")) != -1) &amp;&amp; "{query}" ne "") {
print qq{&lt;item uid="0002"&gt;&lt;arg&gt;configfolder&lt;/arg&gt;&lt;title&gt;Open configuration folder&lt;/title&gt;&lt;subtitle&gt;&lt;/subtitle&gt;&lt;icon&gt;images/config_folder.png&lt;/icon&gt;&lt;/item&gt;\n};}  
